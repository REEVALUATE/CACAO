property_value: dce:date "2025-06-06T16:03:22Z" xsd:dateTime
property_value: skos:definition "Scope note:\nThis class comprises garments worn over other clothes, especially for warmth or protection when outdoors." xsd:string

[Term]
id: CACAO:0000065
