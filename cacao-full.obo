property_value: dce:date "2025-06-06T16:04:39Z" xsd:dateTime
property_value: skos:definition "Scope note:\nThis class comprises a set of outer clothes made of the same fabric and designed to be worn together, typically consisting of at least a jacket and trousers or a skirt." xsd:string

[Term]
id: CACAO:0000071
